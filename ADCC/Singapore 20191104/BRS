#Online

set system host-name BS1OSTC

set vlans vl70 vlan-id 70
set interfaces vlan unit 70 family inet address 193.30.14.250/24
set vlans vl70 l3-interface vlan.70

set interfaces ge-0/0/0.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/1.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/2.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/3.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/4.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/5.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/6.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/7.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/8.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/9.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/10.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/11.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/12.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/13.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/14.0 family ethernet-switching vlan members vl70
set interfaces ge-0/0/15.0 family ethernet-switching vlan members vl70

